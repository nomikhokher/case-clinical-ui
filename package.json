  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "yarn build:web && yarn prisma:generate && yarn build:api",
    "build:api": "nx build api --prod",
    "build:web": "nx build web --prod",
    "dep-graph": "nx dep-graph",
    "dev:api": "nx serve api",
    "dev:sdk": "yarn sdk:web --watch",
    "dev:web": "nx serve web --hmr",
    "docker:build": "docker build . -t schema-driven/api",
    "docker:push": "docker push schema-driven/api",
    "docker:run": "docker run -it -p 8000:3000 schema-driven/api",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && nx lint",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prisma:apply": "yarn prisma:format && yarn prisma:db-push",
    "prisma:db-push": "yarn prisma db push --preview-feature",
    "prisma:format": "yarn prisma format",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate save --experimental && yarn prisma migrate up --experimental",
    "prisma:seed": "yarn nx workspace-generator prisma-seed",
    "prisma:studio": "yarn prisma studio",
    "sdk:web": "graphql-codegen --config libs/web/core/data-access/src/codegen.yml",
    "sdk:web:watch": "yarn sdk:web --watch",
    "setup": "yarn nx workspace-generator workspace-setup",
    "start": "yarn prisma:db-push && node dist/apps/api/main.js",
    "test": "nx test",
    "test:ci": "yarn prisma:apply && yarn prisma:seed --confirm && yarn e2e api-e2e",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
